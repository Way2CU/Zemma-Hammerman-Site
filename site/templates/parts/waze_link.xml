<docuement>
	<!-- hidden input for location object address -->
	<input type="hidden" value="$params['name'][$language]" cms:eval="value"/>
	<!-- Show waze link -->
	<a
		class="waze"
		href="'waze://?q='.$params['name'][$language].'&amp;navigate=yes'"
		cms:eval="href"
		>
		<cms:svg file="site-sprite.svg" symbol="waze"/>
	</a>
</docuement>